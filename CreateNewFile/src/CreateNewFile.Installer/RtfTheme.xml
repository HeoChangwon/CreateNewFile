<?xml version="1.0" encoding="utf-8"?>
<Theme xmlns="http://wixtoolset.org/schemas/thmutil/2010">
  <Window Width="485" Height="300" HexStyle="90CA0000" FontId="0" />

  <Font Id="0" Height="-12" Weight="500" Foreground="000000">굴림</Font>
  <Font Id="1" Height="-24" Weight="500" Foreground="000000">굴림</Font>
  <Font Id="2" Height="-14" Weight="500" Foreground="000000">굴림</Font>

  <Page Name="Help">
    <Text X="11" Y="80" Width="-11" Height="30" FontId="0" DisablePrefix="yes">#(loc.HelpHeader)</Text>
    <Text X="11" Y="112" Width="-11" Height="-35" FontId="0" DisablePrefix="yes">#(loc.HelpText)</Text>
    <Button Name="HelpCancelButton" X="-11" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.HelpCloseButton)</Button>
  </Page>

  <Page Name="Install">
    <Text X="11" Y="80" Width="-11" Height="30" FontId="0" DisablePrefix="yes">#(loc.InstallHeader)</Text>
    <Text X="11" Y="121" Width="60" Height="17" FontId="0" DisablePrefix="yes">#(loc.InstallLabel)</Text>
    <Text X="85" Y="121" Width="-11" Height="17" FontId="0" DisablePrefix="yes">[InstallFolder]</Text>
    <Button Name="OptionsButton" X="11" Y="142" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.InstallOptionsButton)</Button>
    <Button Name="InstallButton" X="-174" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.InstallInstallButton)</Button>
    <Button Name="WelcomeCancelButton" X="-86" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.InstallCloseButton)</Button>
  </Page>

  <Page Name="Options">
    <Text X="11" Y="80" Width="-11" Height="30" FontId="0" DisablePrefix="yes">#(loc.OptionsHeader)</Text>
    <Text X="11" Y="121" Width="60" Height="17" FontId="0" DisablePrefix="yes">#(loc.OptionsLocationLabel)</Text>
    <Editbox Name="FolderEditbox" X="11" Y="143" Width="-91" Height="21" TabStop="yes" FontId="0" FileSystemAutoComplete="yes" />
    <Button Name="BrowseButton" X="-75" Y="142" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.OptionsBrowseButton)</Button>
    <Text X="11" Y="171" Width="-11" Height="51" FontId="0">#(loc.OptionsLocationDescription)</Text>
    <Button Name="OptionsOkButton" X="-174" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.OptionsOkButton)</Button>
    <Button Name="OptionsCancelButton" X="-86" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.OptionsCancelButton)</Button>
  </Page>

  <Page Name="Progress">
    <Text X="11" Y="80" Width="-11" Height="30" FontId="0" DisablePrefix="yes">#(loc.ProgressHeader)</Text>
    <Text Name="OverallProgressPackageText" X="11" Y="121" Width="-11" Height="17" FontId="0" DisablePrefix="yes">#(loc.ProgressLabel)</Text>
    <Progressbar Name="OverallProgressbar" X="11" Y="143" Width="-11" Height="15" />
    <Button Name="ProgressCancelButton" X="-86" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.ProgressCancelButton)</Button>
  </Page>

  <Page Name="Modify">
    <Text X="11" Y="80" Width="-11" Height="30" FontId="0" DisablePrefix="yes">#(loc.ModifyHeader)</Text>
    <Button Name="RepairButton" X="11" Y="143" Width="75" Height="23" TabStop="yes" FontId="0" HideWhenDisabled="yes">#(loc.ModifyRepairButton)</Button>
    <Button Name="UninstallButton" X="99" Y="143" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.ModifyUninstallButton)</Button>
    <Button Name="ModifyCancelButton" X="-86" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.ModifyCloseButton)</Button>
  </Page>

  <Page Name="Success">
    <Text X="11" Y="80" Width="-11" Height="30" FontId="0" DisablePrefix="yes">#(loc.SuccessHeader)</Text>
    <Button Name="LaunchButton" X="11" Y="143" Width="135" Height="23" TabStop="yes" FontId="0" HideWhenDisabled="yes">#(loc.SuccessLaunchButton)</Button>
    <Text Name="SuccessRestartText" X="11" Y="121" Width="-11" Height="34" FontId="0" HideWhenDisabled="yes">#(loc.SuccessRestartText)</Text>
    <Button Name="SuccessRestartButton" X="11" Y="143" Width="135" Height="23" TabStop="yes" FontId="0" HideWhenDisabled="yes">#(loc.SuccessRestartButton)</Button>
    <Button Name="SuccessOkButton" X="-86" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.SuccessCloseButton)</Button>
  </Page>

  <Page Name="Failure">
    <Text X="11" Y="80" Width="-11" Height="30" FontId="0" DisablePrefix="yes">#(loc.FailureHeader)</Text>
    <Text Name="FailureLogFileLink" X="11" Y="121" Width="-11" Height="42" FontId="0" TabStop="yes" HideWhenDisabled="yes">#(loc.FailureHyperlinkLogText)</Text>
    <Text Name="FailureMessageText" X="22" Y="163" Width="-22" Height="51" FontId="0" HideWhenDisabled="yes" />
    <Text Name="FailureRestartText" X="11" Y="121" Width="-11" Height="34" FontId="0" HideWhenDisabled="yes">#(loc.FailureRestartText)</Text>
    <Button Name="FailureRestartButton" X="11" Y="143" Width="135" Height="23" TabStop="yes" FontId="0" HideWhenDisabled="yes">#(loc.FailureRestartButton)</Button>
    <Button Name="FailureOkButton" X="-86" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">#(loc.FailureCloseButton)</Button>
  </Page>

</Theme>